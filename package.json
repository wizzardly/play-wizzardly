{
  "name": "play.wizzard.ly",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "webpack --config webpack.production.js",
    "start": "webpack-dashboard -- webpack-dev-server -d --config webpack.development.js",
    "test": "jest",
    "ci": "jest --coverage && codeclimate-test-reporter < coverage/lcov.info",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'"
