    "@material-ui/icons": "^1.0.0-beta.43",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^8.0.0-beta",
    "prop-types": "^15.5.10",
    "react": "^16.3.2",
    "react-addons-test-utils": "^15.5.1",
