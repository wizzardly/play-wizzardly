    "@babel/preset-stage-0": "^7.0.0-beta.49",
    "@babel/preset-typescript": "^7.0.0-beta.49",
    "@babel/register": "^7.0.0-beta.49",
    "@material-ui/core": "^1.0.0-rc.0",
    "@material-ui/icons": "^1.1.0",
    "@types/jest": "^23.0.0",
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^6.0.2",
    "awesome-typescript-loader": "^5.0.0",
    "axios": "^0.18.0",
    "axios-mock-adapter": "^1.15.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^8.0.0-beta",
    "classnames": "^2.2.5",
    "codeclimate-test-reporter": "^0.5.0",
    "dotenv": "^6.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-matchers": "^6.0.0",
    "faker": "^4.1.0",
    "html-webpack-externals-plugin": "^3.8.0",
    "html-webpack-plugin": "^3.2.0",
    "jest-cli": "^23.1.0",
    "jest-enzyme": "^6.0.0",
    "prop-types": "^15.5.10",
    "react": "^16.3.2",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.4",
    "react-test-renderer": "^16.3.2",
    "redux": "^4.0.0",
    "redux-axios-middleware": "^4.0.0",
    "redux-localstorage": "^0.4.1",
    "redux-mock-store": "^1.5.1",
    "redux-thunk": "^2.2.0",
    "sinon": "^5.0.7",
    "source-map-loader": "^0.2.3",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.1",
    "webpack": "^4.8.3",
    "webpack-cli": "^3.0.3",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-server": "^3.1.4",
    "webpack-git-hash": "^1.0.2",
    "webpack-merge": "^4.1.2",
    "webpack-sentry-plugin": "^1.15.0"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./.jest.js",
    "modulePaths": [
      "app",
      "src"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js",
      "!src/**/index.js",
      "!src/**/styles.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    }
  }
